{
	"settings": {
		"appearanceTools": true,
		"color": {
			"defaultDuotone": false,
			"defaultGradients": false,
			"defaultPalette": false,
			"duotone": null,
			"gradients": null,
			"palette": [
				{
					"color": "#ffffff",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "#000000",
					"name": "Contrast",
					"slug": "contrast"
				},
				{
					"color": "#19484C",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#e45a38",
					"name": "Secondary",
					"slug": "secondary"
				},
				{
					"color": "#FDB63D",
					"name": "Tertiary",
					"slug": "tertiary"
				}
			]
		},
		"custom": {
			"borderRadius": {
				"sm": "4px",
				"md": "8px",
				"lg": "20px",
				"xl": "999px"
			},
			"borderTransparent": "1px solid rgba(53, 80, 48, 0.18)",
			"fontWeight": {
				"black": 900,
				"extraBold": 800,
				"bold": 700,
				"semiBold": 600,
				"medium": 500,
				"regular": 400,
				"light": 300,
				"extraLight": 200,
				"thin": 100
			},
			"lineHeight": {
				"body": 1.75,
				"heading": 1.10,
				"medium": 1.5,
				"one": 1
			}
		},
		"layout": {
			"contentSize": "640px",
			"wideSize": "1200px"
		},
		"spacing": {
			"customSpacingSize": true,
			"spacingScale": {
				"operator": "*",
				"increment": 0,
				"steps": 0,
				"mediumStep": 0,
				"unit": "px"
			},
			"spacingSizes": [
				{
					"name": "XX Small",
					"slug": "xxs",
					"size": "clamp(8px,1vw,10px)"
				},
				{
					"name": "X Small",
					"slug": "xs",
					"size": "var(--wp--preset--font-size--x-small)"
				},
				{
					"name": "Small",
					"slug": "sm",
					"size": "var(--wp--preset--font-size--small)"
				},
				{
					"name": "Medium",
					"slug": "md",
					"size": "var(--wp--preset--font-size--medium)"
				},
				{
					"name": "Large",
					"slug": "lg",
					"size": "var(--wp--preset--font-size--large)"
				},
				{
					"name": "X Large",
					"slug": "xl",
					"size": "var(--wp--preset--font-size--x-large)"
				},
				{
					"name": "XX Large",
					"slug": "xxl",
					"size": "var(--wp--preset--font-size--xx-large)"
				}
			],
			"units": [
				"px",
				"em",
				"rem",
				"vh",
				"vw",
				"%"
			]
		},
		"typography": {
			"dropCap": false,
			"fluid": false,
			"fontFamilies": [
				{
					"fontFace": [
						{
							"fontDisplay": "block",
							"fontFamily": "Plus Jakarta Sans",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "200 800",
							"src": [
								"file:./assets/fonts/PlusJakartaSans-VariableFont_wght.woff2"
							]
						}
					],
					"fontFamily": "Plus Jakarta Sans, sans-serif",
					"name": "Plus Jakarta Sans",
					"slug": "body"
				},
				{
					"fontFace": [
						{
							"fontDisplay": "block",
							"fontFamily": "Syne",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "400 800",
							"src": [
								"file:./assets/fonts/Syne-VariableFont_wght.woff2"
							]
						}
					],
					"fontFamily": "Syne, sans-serif",
					"name": "Syne",
					"slug": "headings"
				}
			],
			"fontSizes": [
				{
					"fluid": {
						"min": "12px",
						"max": "14px"
					},
					"name": "XS",
					"slug": "x-small",
					"size": "clamp(12px, 1.2vw, 14px)"
				},
				{
					"fluid": {
						"min": "14px",
						"max": "16px"
					},
					"name": "S",
					"slug": "small",
					"size": "clamp(14px, 1.6vw, 16px)"
				},
				{
					"name": "M",
					"slug": "medium",
					"size": "clamp(18px, 1.8vw, 20px)",
					"fluid": {
						"min": "18px",
						"max": "20px"
					}
				},
				{
					"fluid": {
						"min": "24px",
						"max": "30px"
					},
					"name": "L",
					"slug": "large",
					"size": "clamp(24px, 3vw, 30px)"
				},
				{
					"fluid": {
						"min": "36px",
						"max": "48px"
					},
					"name": "XL",
					"slug": "x-large",
					"size": "clamp(36px, 3.6vw, 48px)"
				},
				{
					"fluid": {
						"min": "48px",
						"max": "72px"
					},
					"name": "XXL",
					"slug": "xx-large",
					"size": "clamp(48px, 6.6vw, 72px)"
				}
			]
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"blocks": {
			"core/button": {
				"variations": {
					"outline": {
						"spacing": {
							"padding": {
								"bottom": "calc(var(--wp--preset--spacing--xxs) - 2px)",
								"top": "calc(var(--wp--preset--spacing--xxs) - 2px)"
							}
						}
					}
				}
			},
			"core/buttons": {
				"spacing": {
					"blockGap": "var(--wp--preset--spacing--md)"
				}
			},
			"core/site-title": {
				"elements": {
					"link": {
						"typography": {
							"textDecoration": "none"
						}
					}
				},
				"spacing": {
					"margin": {
						"bottom": "0"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)",
					"fontWeight": "var(--wp--custom--font-weight--semi-bold)"
				}
			}
		},
		"color": {
			"background": "var(--wp--preset--color--base)",
			"text": "var(--wp--preset--color--body-copy)"
		},
		"elements": {
			"button": {
				"border": {
					"radius": "var(--wp--custom--border-radius--xl, 999px)"
				},
				"color": {
					"background": "var(--wp--preset--color--primary)",
					"text": "var(--wp--preset--color--base)"
				},
				"spacing": {
					"padding": {
						"bottom": "var(--wp--preset--spacing--xxs)",
						"left": "var(--wp--preset--spacing--lg)",
						"right": "var(--wp--preset--spacing--lg)",
						"top": "var(--wp--preset--spacing--xxs)"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"fontWeight": "var(--wp--custom--font-weight--regular)"
				}
			},
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-large)"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--headings)",
					"lineHeight": "var(--wp--custom--line-height--heading)"
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--body-copy)"
				}
			}
		},
		"spacing": {
			"blockGap": "var(--wp--preset--spacing--lg)",
			"padding": {
				"bottom": "0px",
				"left": "var(--wp--preset--spacing--md)",
				"right": "var(--wp--preset--spacing--md)",
				"top": "0px"
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--body)",
			"fontSize": "var(--wp--preset--font-size--small)",
			"fontWeight": "var(--wp--custom--font-weight--light)",
			"lineHeight": "var(--wp--custom--line-height--body)"
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header",
			"title": "Header"
		},
		{
			"area": "footer",
			"name": "footer",
			"title": "Footer"
		}
	],
	"version": 2,
	"$schema": "https://schemas.wp.org/wp/6.1/theme.json"
}